package com.employee;

public class GroupingBy {
   
	public static void main(String [] args) {
		String name="Cloud computing";
		
	String newValue=	replaceStringCharacter(name);
	System.out.println(newValue);
	}
	public static	String replaceStringCharacter(String name){
		String newString="";
		for(int i=0;i<name.length();i++) {
			if(name.charAt(i)=='c' || name.charAt(i)=='C') 
				continue;
			newString=newString+name.charAt(i);
			
		}
		return newString;	
		}
}
